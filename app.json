{
  "expo": {
    "name": "Stamper",
    "slug": "stamper-app",
    "sdkVersion": "36.0.0",
    "scheme": "stamper",
    "privacy": "unlisted",
    "orientation": "portrait",
    "description": "Stamper to wirtualny portfel kart lojalnościowych w Twoim telefonie - zawsze gotowy by zdobyć nowe pieczątki. Aplikacja zrzesza bardziej lojalnych klientów lubiących korzystać z kart lojalnościowych. Uwolnij się od noszenia wielu papierowych kart i tracenia możliwości zbierania pieczątek podczas zamawiania jedzenia do domu. Od teraz już nigdy nie zgubisz kart lojalnościowych!",
    "primaryColor": "#0046F4",
    "version": "1.9.2",
    "platforms": ["ios", "android"],
    "facebookAppId": "356151625034848",
    "facebookDisplayName": "Stamper",
    "assetBundlePatterns": ["**/*"],
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#001432"
    },
    "updates": {
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    },
    "hooks": {
      "postPublish": [
        {
          "file": "expo-postpublish-rollbar",
          "config": {
            "serverItemAccessToken": "64cee778687d4cdeb209a00c8b66eb62",
            "minifiedUrlTemplate": "http://reactnativehost/{{url}}"
          }
        }
      ]
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.getstamper.app",
      "config": {
        "googleMapsApiKey": "AIzaSyDTfi_GPZda7SSDW9oXKbtrKJHSbqCh2m0"
      },
      "infoPlist": {
        "NSCameraUsageDescription": "Stamper needs your camera to allow you to scan QR codes and get rewarded.",
        "NSLocationWhenInUseUsageDescription": "Stamper needs your location to help you find nearby promotions."
      }
    },
    "android": {
      "package": "com.getstamper.app",
      "versionCode": 12,
      "adaptiveIcon": {
        "backgroundColor": "#001432"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDTfi_GPZda7SSDW9oXKbtrKJHSbqCh2m0"
        }
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    }
  }
}
